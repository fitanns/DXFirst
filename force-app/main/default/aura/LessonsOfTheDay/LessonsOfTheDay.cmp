<aura:component description="LessonsOfTheDay">

    <aura:attribute name="hour" type="String"/>
    <div class="main-table">
        <table class="day-table">
            <tbody>
            <tr>
                <td class="day-hours" aura:id="{!v.hour}">{!v.hour}</td>
                <td class="time-lesson">asd</td>
            </tr>
            <tr>
                <td class="day-hours"></td>
                <td class="time-lesson">a</td>
            </tr>
            </tbody>
        </table>
    </div>
</aura:component>
